       RowBox[{
        RowBox[{"-", 
         RowBox[{"\[Gamma]", "^", "2"}]}], "/", "Px"}], ",", 
       RowBox[{
        RowBox[{"-", 
         RowBox[{"\[Gamma]", "^", "2"}]}], "/", "Py"}]}], "}"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"gradPhi", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{"-", 
         RowBox[{"Sin", "[", "\[Phi]", "]"}]}], "/", 
        RowBox[{"(", 
         RowBox[{"\[Gamma]", " ", 
          RowBox[{"Sqrt", "[", 
           RowBox[{"Px", "*", "Py"}], "]"}]}], ")"}]}], ",", 
       RowBox[{
        RowBox[{"Cos", "[", "\[Phi]", "]"}], "/", 
        RowBox[{"(", 
         RowBox[{"\[Gamma]", " ", 
          RowBox[{"Sqrt", "[", 
           RowBox[{"Px", "*", "Py"}], "]"}]}], ")"}]}], ",", "0", ",", "0"}], 
