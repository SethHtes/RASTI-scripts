 RowBox[{
  RowBox[{"(*", 
   RowBox[{"Gradients", " ", "at", " ", "true", " ", "values"}], "*)"}], 
  RowBox[{
   RowBox[{
    RowBox[{"gradGamma2", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"2", "\[Gamma]", " ", 
        RowBox[{
         RowBox[{"Cos", "[", "\[Phi]", "]"}], "/", 
         RowBox[{"Sqrt", "[", 
          RowBox[{"Px", "*", "Py"}], "]"}]}]}], ",", 
       RowBox[{"2", "\[Gamma]", " ", 
        RowBox[{
         RowBox[{"Sin", "[", "\[Phi]", "]"}], "/", 
         RowBox[{"Sqrt", "[", 
